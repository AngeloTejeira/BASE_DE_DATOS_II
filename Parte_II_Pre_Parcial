-- Creaci칩n de tablas
CREATE TABLE Usuarios (
  usuario_id INT PRIMARY KEY,
  nombre VARCHAR(50),
  nombre_usuario VARCHAR(50),
  contrase침a VARCHAR(50),
  rol VARCHAR(50)
);

CREATE TABLE Aseguradoras (
  aseguradora_id INT PRIMARY KEY,
  nombre VARCHAR(50)
);

CREATE TABLE Juzgados (
  juzgado_id INT PRIMARY KEY,
  nombre VARCHAR(50),
  ubicacion VARCHAR(100)
);

CREATE TABLE Expedientes (
  expediente_id INT PRIMARY KEY,
  id_expediente INT,
  conductor VARCHAR(50),
  aseguradora_id INT,
  numero_caso VARCHAR(20),
  estado INT,
  fecha_creacion DATE,
  usuario_id INT,
  juzgado_id INT,
  FOREIGN KEY (aseguradora_id) REFERENCES Aseguradoras (aseguradora_id),
  FOREIGN KEY (usuario_id) REFERENCES Usuarios (usuario_id),
  FOREIGN KEY (juzgado_id) REFERENCES Juzgados (juzgado_id)
);

CREATE TABLE Estados (estado_id INT PRIMARY KEY, nombre VARCHAR(50));

CREATE TABLE Reportes (
  reporte_id INT PRIMARY KEY,
  expediente_id INT,
  descripcion TEXT,
  fecha DATE,
  FOREIGN KEY (expediente_id) REFERENCES Expedientes (expediente_id)
);

-- Creaci칩n de relaciones
ALTER TABLE Expedientes ADD FOREIGN KEY (estado) REFERENCES Estados (estado_id);
